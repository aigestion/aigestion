{
  "name": "44-twilio-auto-responder",
  "nodes": [
    {
      "parameters": {
        "path": "sms-webhook",
        "options": {}
      },
      "name": "SMS Trigger",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1,
      "position": [250, 300]
    },
    {
      "parameters": {
        "from": "{{$env[\"TWILIO_NUMBER\"]}}",
        "to": "={{$node[\"SMS Trigger\"].json[\"body\"][\"From\"]}}",
        "message": "Thanks for your message! Our support team will get back to you shortly."
      },
      "name": "Send Reply",
      "type": "n8n-nodes-base.twilio",
      "typeVersion": 1,
      "position": [450, 300]
    }
  ],
  "connections": {
    "SMS Trigger": {
      "main": [
        [
          {
            "node": "Send Reply",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
}
