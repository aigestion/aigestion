{
  "name": "65-ad-spend-tracker",
  "nodes": [
    {
      "parameters": {
        "rule": {
          "interval": [
            {
              "field": "days",
              "triggerAtHour": 8
            }
          ]
        }
      },
      "name": "Daily",
      "type": "n8n-nodes-base.cron",
      "typeVersion": 1,
      "position": [250, 300]
    },
    {
      "parameters": {
        "jsCode": "return [\n  { json: { platform: 'Facebook', spend: 120.50, conversions: 5 } },\n  { json: { platform: 'Google', spend: 210.00, conversions: 8 } }\n];"
      },
      "name": "Mock Ads API",
      "type": "n8n-nodes-base.code",
      "typeVersion": 1,
      "position": [450, 300]
    },
    {
      "parameters": {
        "operation": "append",
        "sheetId": "SHEET_ID_HERE",
        "range": "A:C",
        "options": {}
      },
      "name": "Update Sheets",
      "type": "n8n-nodes-base.googleSheets",
      "typeVersion": 1,
      "position": [650, 300]
    }
  ],
  "connections": {
    "Daily": {
      "main": [
        [
          {
            "node": "Mock Ads API",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Mock Ads API": {
      "main": [
        [
          {
            "node": "Update Sheets",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
}
