{
  "name": "88-weekly-goal-tracker",
  "nodes": [
    {
      "parameters": {
        "rule": {
          "interval": [
            {
              "field": "weeks",
              "triggerAtDay": [
                1,
                5
              ],
              "triggerAtHour": 9
            }
          ]
        }
      },
      "name": "Mon/Fri 9AM",
      "type": "n8n-nodes-base.cron",
      "typeVersion": 1,
      "position": [250, 300]
    },
    {
      "parameters": {
        "channel": "general",
        "text": "=Week START/END Check-in! \nWhat are your top 3 goals (Mon) / What did you achieve (Fri)?",
        "otherOptions": {}
      },
      "name": "Slack Prompt",
      "type": "n8n-nodes-base.slack",
      "typeVersion": 1,
      "position": [450, 300]
    }
  ],
  "connections": {
    "Mon/Fri 9AM": {
      "main": [
        [
          {
            "node": "Slack Prompt",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
}
