{
  "name": "11-sync-sales-data",
  "nodes": [
    {
      "parameters": {
        "rule": {
          "interval": [
            {
              "field": "hours",
              "triggerAtHour": 8
            }
          ]
        }
      },
      "name": "Every Morning",
      "type": "n8n-nodes-base.cron",
      "typeVersion": 1,
      "position": [250, 300]
    },
    {
      "parameters": {
        "operation": "getAll",
        "limit": 50,
        "options": {}
      },
      "name": "Salesforce Get",
      "type": "n8n-nodes-base.salesforce",
      "typeVersion": 1,
      "position": [450, 300]
    },
    {
      "parameters": {
        "operation": "append",
        "sheetId": "sales-data",
        "options": {}
      },
      "name": "Google Sheets",
      "type": "n8n-nodes-base.googleSheets",
      "typeVersion": 1,
      "position": [650, 300]
    }
  ],
  "connections": {
    "Every Morning": {
      "main": [
        [
          {
            "node": "Salesforce Get",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Salesforce Get": {
      "main": [
        [
          {
            "node": "Google Sheets",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
}
