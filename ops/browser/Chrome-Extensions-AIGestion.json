    "aigestion_specific": [
      {
        "id": "custom_aigestion_dev",
        "name": "AIGestion Dev Tools",
        "description": "Herramientas personalizadas para desarrollo AIGestion",
        "category": "aigestion",
        "priority": "high",
        "enabled": true,
        "custom": true,
        "manifest": {
          "name": "AIGestion Dev Tools",
          "version": "1.0.0",
          "manifest_version": 3,
          "description": "Herramientas de desarrollo específicas para AIGestion",
          "permissions": ["activeTab", "storage", "scripting"],
          "background": {
            "service_worker": "background.js"
          },
          "content_scripts": [
            {
              "matches": ["*://*.aigestion.net/*", "http://localhost:*/*"],
              "js": ["content.js"]
            }
          ],
          "action": {
            "default_popup": "popup.html",
            "default_title": "AIGestion Dev Tools"
          }
        }
      }
    ]
  },
  "profiles": {
    "development": {
      "name": "AIGestion Development",
      "extensions": ["development", "aigestion_specific"],
      "flags": [
        "enable-gpu-rasterization",
        "enable-zero-copy",
        "enable-oop-rasterization",
        "enable-skia-renderer",
        "enable-quic",
        "disable-web-security",
        "ignore-certificate-errors"
      ]
    },
    "admin": {
      "name": "AIGestion Admin",
      "extensions": ["development", "productivity", "security", "aigestion_specific"],
      "flags": [
        "enable-gpu-rasterization",
        "enable-zero-copy",
        "enable-quic",
        "disable-web-security",
        "ignore-certificate-errors",
        "disable-features=IsolateOrigins,site-per-process"
      ]
    },
    "production": {
      "name": "AIGestion Production",
      "extensions": ["security", "productivity"],
      "flags": ["enable-gpu-rasterization", "enable-zero-copy", "enable-quic"]
    }
  },
  "installation": {
    "auto_install": true,
    "skip_permissions": false,
    "enable_developer_mode": true,
    "load_unpacked": true
  },
  "configuration": {
    "sync_enabled": false,
    "auto_update": true,
    "notifications": {
      "extension_updates": true,
      "security_alerts": true,
      "performance_warnings": true
    }
  }
}
