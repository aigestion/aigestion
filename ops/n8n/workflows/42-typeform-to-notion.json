{
  "name": "42-typeform-to-notion",
  "nodes": [
    {
      "parameters": {
        "httpMethod": "POST",
        "path": "typeform-submit",
        "options": {}
      },
      "name": "Webhook",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1,
      "position": [250, 300]
    },
    {
      "parameters": {
        "resource": "database",
        "operation": "create",
        "databaseId": "{{$env[\"NOTION_SURVEY_DB_ID\"]}}",
        "propertiesUi": {
          "property": [
            {
              "name": "Name",
              "value": "={{$node[\"Webhook\"].json[\"body\"][\"form_response\"][\"answers\"][0][\"text\"]}}",
              "type": "title"
            },
            {
              "name": "Email",
              "value": "={{$node[\"Webhook\"].json[\"body\"][\"form_response\"][\"answers\"][1][\"email\"]}}",
              "type": "email"
            },
            {
              "name": "Feedback",
              "value": "={{$node[\"Webhook\"].json[\"body\"][\"form_response\"][\"answers\"][2][\"text\"]}}",
              "type": "rich_text"
            }
          ]
        }
      },
      "name": "Create Item",
      "type": "n8n-nodes-base.notion",
      "typeVersion": 1,
      "position": [450, 300]
    }
  ],
  "connections": {
    "Webhook": {
      "main": [
        [
          {
            "node": "Create Item",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
}
