# .env.example
# Copy this file to .env and replace placeholder values with real secrets

# 1️⃣ SERVER & RUNTIME
NODE_ENV=development
PORT=3000
FRONTEND_URL=https://www.aigestion.net
BASE_URL=https://api.aigestion.net
CORS_ORIGIN=http://localhost:5173,http://localhost:3000,http://localhost:4001,https://www.aigestion.net

# 2️⃣ SECURITY & AUTHENTICATION
JWT_SECRET=YOUR_JWT_SECRET
JWT_EXPIRES_IN=7d
JWT_COOKIE_EXPIRES_IN=7
SESSION_SECRET=YOUR_SESSION_SECRET
API_KEY=YOUR_API_KEY
N8N_JWT_TOKEN=YOUR_N8N_JWT_TOKEN
RECAPTCHA_SITE_KEY=YOUR_RECAPTCHA_SITE_KEY
RECAPTCHA_SECRET_KEY=YOUR_RECAPTCHA_SECRET_KEY
IAP_EXPECTED_AUDIENCE=YOUR_IAP_EXPECTED_AUDIENCE
IAP_ENABLED=false

# 3️⃣ RATE LIMITING
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=100
AUTH_RATE_LIMIT_WINDOW_MS=3600000
AUTH_RATE_LIMIT_MAX=10
AI_RATE_LIMIT_WINDOW_MS=600000
AI_RATE_LIMIT_MAX=30

# 4️⃣ DATABASES, QUEUES & CACHING (placeholders)
MONGODB_URI=YOUR_MONGODB_URI
MONGO_ROOT_USER=YOUR_MONGO_ROOT_USER
MONGO_ROOT_PASSWORD=YOUR_MONGO_ROOT_PASSWORD
REDIS_HOST=YOUR_REDIS_HOST
REDIS_PORT=6379
REDIS_PASSWORD=YOUR_REDIS_PASSWORD
REDIS_URL=YOUR_REDIS_URL
POSTGRES_USER=YOUR_POSTGRES_USER
POSTGRES_PASSWORD=YOUR_POSTGRES_PASSWORD
POSTGRES_DB=YOUR_POSTGRES_DB
DATABASE_URL=YOUR_DATABASE_URL
RABBITMQ_DEFAULT_USER=YOUR_RABBITMQ_USER
RABBITMQ_DEFAULT_PASS=YOUR_RABBITMQ_PASS
RABBITMQ_URI=YOUR_RABBITMQ_URI

# 5️⃣ AI ENGINE (ML SPECIFIC)
APP_NAME=AIG-ia-engine
IA_ENGINE_URL=YOUR_IA_ENGINE_URL
APP_DEBUG=true
APP_WORKERS=4
MODEL_PATH=./data/trained_models
DEFAULT_MODEL=hope-lite-v1
MAX_BATCH_SIZE=32
INFERENCE_TIMEOUT=30
ONNX_ENABLED=true
ONNX_THREADS=4
ONNX_USE_GPU=false
TRAINING_EPOCHS=10
LEARNING_RATE=0.001
CHECKPOINT_DIR=./data/checkpoints
UPLOAD_DIR=./data/uploads
MAX_UPLOAD_SIZE=10485760
ENABLE_RETRAINING=true
ENABLE_RAG=false
ENABLE_ANOMALY_DETECTION=true

# 6️⃣ AI PROVIDERS (LLM & TOOLS)
OPENAI_API_KEY=YOUR_OPENAI_API_KEY
ANTHROPIC_API_KEY=YOUR_ANTHROPIC_API_KEY
GEMINI_API_KEY=YOUR_GEMINI_API_KEY
