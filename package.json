{
  "name": "aigestion.net",
  "displayName": "AIGestion.net",
  "description": "Arquitectura de Inteligencia Soberana - Development Environment",
  "private": true,
  "version": "2.0.0",
  "packageManager": "pnpm@10.28.2",
  "homepage": "https://aigestion.net",
  "repository": {
    "type": "git",
    "url": "https://github.com/aigestion/aigestion-net.git"
  },
  "author": {
    "name": "AIGestion Team",
    "email": "dev@aigestion.net"
  },
  "scripts": {
    "parse": "ts-node tests/parse/index.ts",
    "test": "jest --config .config/jest.config.cjs",
    "build": "npx --yes turbo build --filter=aigestion-website-epic... --filter=aigestion-client-dashboard... --filter=aigestion-admin-dashboard... --filter=aigestion-demo-dashboard...",
    "trunk": "trunk",
    "lint": "eslint . --config .config/eslint.config.cjs",
    "format": "prettier --write . --config .config/.prettierrc",
    "prepare": "husky",
    "push:hourly": "ts-node ./ops/scripts/push_repo.ts --hourly",
    "dev:backend": "cd backend && pnpm install && pnpm run dev",
    "dev:frontend": "cd frontend && pnpm install && pnpm run dev:all",
    "dev:ml": "cd ml-service && powershell ./start-service.ps1",
    "dev:swarm": "cd aig-ia-engine/backend && uvicorn app.main:app --host 0.0.0.0 --port 8005",
    "dev:docker": "docker-compose up -d --build",
    "dev": "concurrently \"pnpm run dev:backend\" \"pnpm run dev:frontend\" \"pnpm run dev:ml\" \"pnpm run dev:swarm\"",
    "test:full": "pnpm run test --workspaces && pnpm run test --prefix backend && pnpm run test --prefix frontend",
    "test:coverage": "turbo run test:coverage",
    "test:complete": "bash ops/scripts/test-complete.sh",
    "test:unit": "bash ops/scripts/test-complete.sh unit",
    "test:integration": "bash ops/scripts/test-complete.sh integration",
    "test:e2e": "bash ops/scripts/test-complete.sh e2e",
    "test:performance": "bash ops/scripts/test-complete.sh performance",
    "test:security": "bash ops/scripts/test-complete.sh security",
    "test:ai": "bash ops/scripts/test-complete.sh ai",
    "test:full:seq": "pnpm run test -- --runInBand && pnpm run test --prefix backend -- --runInBand",
    "sonar": "sonar-scanner --config .config/sonar-project.properties",
    "deploy": "echo 'Deployment handled by GitHub Actions CI/CD pipeline'",
    "deploy:github-pages": "echo 'GitHub Pages deployment triggered via GitHub Actions'",
    "setup": "pnpm install && cd frontend && pnpm install && cd ../backend && pnpm install",
    "setup:complete": "bash ops/scripts/setup-complete.sh",
    "setup:check": "bash ops/scripts/setup-complete.sh health",
    "setup:monitoring": "bash ops/scripts/setup-monitoring.sh",
    "monitoring:status": "bash ops/scripts/monitoring-status.sh",
    "monitoring:cleanup": "bash ops/scripts/monitoring-cleanup.sh",
    "monitoring:restart": "docker-compose -f docker-compose.monitoring.yml restart",
    "deploy:multi-region": "echo 'Multi-region deployment handled by GitHub Actions'",
    "multi-region:status": "bash ops/scripts/multi-region-status.sh",
    "multi-region:lag": "bash ops/scripts/check-replication-lag.sh",
    "multi-region:dns": "bash ops/scripts/deploy-multi-region.sh dns",
    "multi-region:monitoring": "bash ops/scripts/deploy-multi-region.sh monitoring",
    "clean": "pnpm run clean:frontend && pnpm run clean:backend",
    "clean:frontend": "cd frontend && rm -rf node_modules dist && pnpm install",
    "clean:backend": "cd backend && rm -rf node_modules dist && pnpm install",
    "clean:processes": "powershell \"Get-Process | Where-Object { $_.Name -match 'node|jest|mongod' } | Stop-Process -Force -ErrorAction SilentlyContinue\"",
    "git:sync": "git add . && git commit -m 'Auto-sync AIGestion.net' && git push origin main",
    "docker:build": "docker build -t aigestion-net .",
    "docker:run": "docker run -p 3000:3000 aigestion-net",
    "knowledge:sync": "pnpm --prefix backend run sync:knowledge",
    "dashboard:init": "pnpm --prefix backend run init:dashboard"
  },
  "devDependencies": {
    "@aigestion/eslint-config": "file:packages/eslint-config",
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@capacitor/cli": "^8.0.2",
    "@jest/console": "^29.7.0",
    "@jest/reporters": "^29.7.0",
    "@jest/test-result": "^29.7.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@trunkio/launcher": "^1.3.4",
    "@types/caller": "^1.0.2",
    "@types/debug": "^4.1.12",
    "@types/jest": "^29.5.14",
    "@types/jest-specific-snapshot": "^0.5.9",
    "@types/node": "^22.19.3",
    "@types/semver": "^7.7.0",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "autoprefixer": "10.4.24",
    "babel-jest": "^29.7.0",
    "caller": "^1.1.0",
    "chalk": "^4.1.2",
    "concurrently": "^9.2.1",
    "debug": "^4.4.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-import-x": "^4.13.3",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-n": "^17.18.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "fast-sort": "^3.4.1",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-extended": "^7.0.0",
    "jest-junit": "^16.0.0",
    "jest-specific-snapshot": "^8.0.0",
    "lint-staged": "^15.5.2",
    "postcss": "8.5.6",
    "semver": "^7.7.2",
    "simple-git": "^3.27.0",
    "sonarqube-scanner": "^4.3.4",
    "syncpack": "^13.0.4",
    "tailwindcss": "4.1.18",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.5.1",
    "turbo": "^2.1.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0",
    "vite": "5.4.21",
    "yaml": "^2.8.0",
    "zod": "^3.22.4"
  },
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "@capacitor/android": "^8.0.2",
    "@capacitor/core": "^8.0.2",
    "@supabase/supabase-js": "^2.93.1",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "pnpm": {
    "overrides": {
      "axios": "^1.13.5",
      "fast-xml-parser": "^5.3.4",
      "tar": "^6.2.1",
      "cross-spawn": "^7.0.5",
      "nanoid": "^3.3.8",
      "@isaacs/brace-expansion": "^5.0.1",
      "protobufjs": "^7.2.5",
      "esbuild@<=0.24.2": ">=0.25.0",
      "cookie@<0.7.0": ">=0.7.0",
      "tar@<=7.5.2": ">=7.5.3",
      "tar@<=7.5.3": ">=7.5.4",
      "lodash@>=4.0.0 <=4.17.22": ">=4.17.23",
      "undici@<6.23.0": ">=6.23.0",
      "tar@<7.5.7": ">=7.5.7",
      "diff@>=4.0.0 <4.0.4": ">=4.0.4",
      "js-yaml@>=4.0.0 <4.1.1": ">=4.1.1",
      "qs@>=6.7.0 <=6.14.1": ">=6.14.2",
      "markdown-it": ">=14.1.1"
    }
  }
}