    /**
     * Limpiar contextos antiguos
     */
    cleanup() {
        const now = new Date();
        const MAX_AGE_MS = 24 * 60 * 60 * 1000; // 24 horas
        for (const [chatId, context] of this.contexts.entries()) {
            if (now.getTime() - context.lastUpdate.getTime() > MAX_AGE_MS) {
                this.contexts.delete(chatId);
            }
        }
    }
