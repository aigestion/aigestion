"use strict";
Object.defineProperty(exports, "__esModule", { value: true });
exports.buildResponse = buildResponse;
exports.buildError = buildError;
function buildResponse(data, statusCode = 200, requestId) {
    return {
        status: statusCode,
        data,
        timestamp: new Date().toISOString(),
        requestId,
    };
}
/**
 * Build an error response.
 */
function buildError(message, code, statusCode = 400, requestId, details) {
    return {
        status: statusCode,
        error: {
            code,
            message,
            timestamp: new Date().toISOString(),
            requestId,
            details,
        },
    };
}
